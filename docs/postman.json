{
    "info": {
        "name": "Paid-BE API Collection",
        "description": "Complete API collection for Paid Backend - Bill splitting with face detection, expense management, and payment tracking",
        "version": "2.0.0",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "auth": {
        "type": "bearer",
        "bearer": [
            {
                "key": "token",
                "value": "{{auth_token}}",
                "type": "string"
            }
        ]
    },
    "variable": [
        {
            "key": "baseUrl",
            "value": "http://localhost:3000/api",
            "type": "string"
        },
        {
            "key": "auth_token",
            "value": "",
            "type": "string",
            "description": "JWT token for authenticated user"
        },
        {
            "key": "session_id",
            "value": "",
            "type": "string",
            "description": "Current session UUID"
        },
        {
            "key": "player_id",
            "value": "",
            "type": "string",
            "description": "Current player UUID"
        },
        {
            "key": "obligation_id",
            "value": "",
            "type": "string",
            "description": "Current obligation UUID"
        },
        {
            "key": "payment_id",
            "value": "",
            "type": "string",
            "description": "Current payment UUID"
        },
        {
            "key": "expense_id",
            "value": "",
            "type": "string",
            "description": "Current expense UUID"
        },
        {
            "key": "magic_link_token",
            "value": "",
            "type": "string",
            "description": "Magic link token for player access"
        }
    ],
