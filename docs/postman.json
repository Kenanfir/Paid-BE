                        "type": "noauth"
                    },
                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"email\": \"felly@example.com\",\n  \"password\": \"password123\",\n  \"name\": \"Felly\",\n  \"phone\": \"+62811111111\"\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/auth/register",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "auth",
                                "register"
                            ]
                        }
                    },
                    "event": [
                        {
                            "listen": "test",
                            "script": {
                                "exec": [
                                    "if (pm.response.code === 201) {",
                                    "  const response = pm.response.json();",
                                    "  pm.collectionVariables.set('auth_token', response.data?.token || '');",
                                    "}"
