                    "request": {
                        "method": "POST",
                        "header": [
                            {
                                "key": "Content-Type",
                                "value": "application/json"
                            }
                        ],
                        "body": {
                            "mode": "raw",
                            "raw": "{\n  \"token\": \"{{magic_link_token}}\"\n}"
                        },
                        "url": {
                            "raw": "{{baseUrl}}/auth/magic-link/verify",
                            "host": [
                                "{{baseUrl}}"
                            ],
                            "path": [
                                "auth",
                                "magic-link",
                                "verify"
                            ]
                        }
                    }
                }
            ]
